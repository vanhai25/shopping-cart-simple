RewriteEngine on

RewriteRule ^trang-chu$ index.php?page=home 

RewriteRule ^gio-hang$ index.php?page=cart 
RewriteRule ^thanh-toan$ index.php?page=checkout  
RewriteRule ^dang-nhap$ index.php?page=login  
RewriteRule ^dang-ky$ index.php?page=register 




RewriteRule ^([a-zA-Z0-9_-]+).shop$ page.php?shop=$1
RewriteRule ^([a-zA-Z0-9_-]+).shop/([0-9]+)$ page.php?shop=$1&page=$2
RewriteRule ^([a-zA-Z0-9_-]+).shop/([a-zA-Z0-9_-]+)$ page.php?shop=$1&menu=$2
RewriteRule ^([a-zA-Z0-9_-]+).shop/([a-zA-Z0-9_-]+)/([0-9]+)$ page.php?shop=$1&menu=$2&page=$3

RewriteRule ^([a-zA-Z0-9_-]+)$ typeProduct.php?url=$1
RewriteRule ^([a-zA-Z0-9-]+)/([0-9]+)$ typeProduct.php?url=$1&page=$2
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)-([0-9]+).html$ product.php?url=$1&title=$2&id=$3


